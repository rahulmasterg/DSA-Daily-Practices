import java.io.*;
import java.util.*;

class Main
{
   public static int fib(int n)
   {
      if(n==1){
          return 0;
      }   

      if(n==2){
          return 1;
      }

       int sum = fib(n-1) + fib(n-2);

       return sum;
   }

   public static void main(String args[])throws IOException
   {
      Scanner sc = new Scanner(System.in);
      int n=sc.nextInt();

      System.out.println(fib(n));
   }
}

/*
Find nth element in fibonacci series

Input: 7
Output: 8

*/
