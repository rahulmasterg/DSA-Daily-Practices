import java.util.*;
import java.lang.*;
import java.io.*;

// The main method must be in a class named "Main".
class Node{
    int data;
    Node next;

    Node(int data){
        this.data = data;
        this.next = null;
    }
}

class Main {
    static Node insertNode(Node head, int n, int pos, int val){
        if(pos==0){
            Node temp = new Node(val);
            temp.next = head;
            head = temp;
        }else if(pos==n){
            Node temp = new Node(val);

            Node tail = head;

            for(int i=0; i<n-1; i++){
                tail = tail.next;
            }

            tail.next = temp;
        }else{
            Node itr = head;

            for(int i=0; i<pos-1; i++){
                itr = itr.next;
            }
            Node newNode = new Node(val);

            newNode.next = itr.next;

            itr.next = newNode;
        }
        return head;
    }
    static void printList(Node head){
        while(head!=null){
            System.out.print(head.data+" ");
            head = head.next;
        }
    }
    public static void main(String[] args) {
        //System.out.println("Hello world!");
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        Node head = new Node(sc.nextInt());

        Node tail = head;

        for(int i=1; i<n; i++){
            Node curr = new Node(sc.nextInt());

            tail.next = curr;
            tail = curr;
        }
        int pos = sc.nextInt();
        int val = sc.nextInt();
        
        insertNode(head, n, pos, val); //for 1 based indexing pos = pos-1
        printList(head);
    }
}
